<style>
</style>

<template>
  <web-view src="{{url}}"></web-view>
</template>

<script>
import wepy from 'wepy';

export default class Webview extends wepy.page {
  onLoad(options) {
    var that = this;
    //console.log(options);
    that.setData({
      url: options.link
    });
    wx.setNavigationBarTitle({
      title: options.title
    });
  }

  config = {
    navigationBarTitleText: 'web-view'
  };
}
</script>


